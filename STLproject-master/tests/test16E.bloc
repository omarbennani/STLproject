// La classe Honda doit impl√©menter laver() et detruitre()

interface Vehicule {
    void laver();
}

interface Voiture extends Vehicule {
    void detruire();
}

class Honda implements Voiture {
    int serie;
    public Voiture(int s){
        serie = s;
    }
    public void laver() {
        serie = 0;
    }
    public void detruire(int i) {
        serie = 10;
    }
}

public class Main {
	public static void main(String [] args) {
	    Vehicule v = new Honda(2);
	}
}
