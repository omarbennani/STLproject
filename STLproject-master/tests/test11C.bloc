// Une interface + une classe impl√©mentant l'interface

interface Vehicule {
    void laver();
}

class Voiture extends Vehicule {
    private int serie;
    public Voiture(int s){
  	    serie = s;
    }
    public void laver(){
  	    this.serie = 0;
    }
}
public class Main {
	public static void main( String [] args) {
	    Voiture v = new Voiture(2);
	    v.laver();
	}
}
