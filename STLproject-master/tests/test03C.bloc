// Une classe avec un constructeur, un attribut privé et une méthode void

class Voiture {
    private int serie;
    public Voiture(int t) {
        serie = t;
    }
    public void setSerie( int _serie) {
    	this.serie = _serie;
    }
}

public class Main {
	public static void main( String [] args) {
	    Voiture v = new Voiture(2);
        v.setSerie(1);
	}
}
