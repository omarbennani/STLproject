// Erreur
// Tentative d'accès à la methode afficher via un objet alors qu'elle est statique

class Voiture {

    private int serie;

    public Voiture(int t) {
        serie = t;
    }
    public static void afficher(int _serie) {
    	print(_serie);
    }
}

public class Main {
	public static void main( String [] args) {
	    Voiture v = new Voiture(2);
        v.afficher(3);
	}
}
