// Erreur
// Tentative d'accès à la methode setSerie qui est privée

class Voiture {

    private int serie;

    public Voiture(int t) {
        serie = t;
    }
    private void setSerie( int _serie) {
    	serie = _serie;
    }
}

public class Main {
	public static void main( String [] args) {
	    Voiture v = new Voiture(2);
        v.setSerie(3);
	}
}
