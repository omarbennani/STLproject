ElementClasse -> statique AttributOuMethode #ast;
#ast {
local
	a : ElementClasse;
do
    a := AttributOuMethode^ast;
    a.setStatic(true);

	ElementClasse^ast := a;

	if AttributOuMethode^typeElementClasse = 0 then
