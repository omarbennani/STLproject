        d := f.get();
		match d
            with Classe then
                SuiteHeritageGenericite^ast.addFirst(d.getType());
                HeritageGenericite^ast := SuiteHeritageGenericite^ast;
            else
                error(BLOC_not_an_object, identificateur_type^txt);
